const updateConfig = (
  component_n: string,
  configTitle: string,
  value: string | boolean | string[]
) => {
  setRAGConfig((prevRAGConfig) => {
    if (!prevRAGConfig) return null;

    const newRAGConfig = { ...prevRAGConfig };
    const component = newRAGConfig[component_n as keyof RAGConfig];

    // Проверяем, что component и component.config существуют, и что configTitle является ключом в component.config
    if (component && component.config && configTitle in component.config) {
      const configItem = component.config[configTitle]; // Правильный доступ к свойству по ключу

      configItem.value = value;
    }
    return newRAGConfig;
  });
};
